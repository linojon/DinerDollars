
<div class="container">
  <h2>My Dedicated Diner Dollars Review</h2>
  <br />
  <table class="table table-striped table-bordered">
    <tr class="success">
      <th>Restaurant</th>
      <th>Points</th>
      <th>Diner $ Available</th>
      <th>Points Needed to Next $</th>
    </tr>
  <tr class="success">
    <% @shops.each do |shop| %>
      <td>shop.name</td>
      <td>shop.points</td>
      <td>Diner $ Available</td>
      <td>Points Needed to Next $</td>
    </tr>
    <% end %>
  </table>
</div>
