<h1>Enrollments#show</h1>
<div class="container">
  <h1>My Dedicated Diner Dollars Review</h1>
  <br />
  <table class="table table-striped table-bordered">
    <tr class="success">
      <th>Restaurant</th>
      <th>Points</th>
      <th>Diner $ Available</th>
      <th>Points Needed to Next $</th>
    </tr>
  
    <% @enrollments.each do |enrollment| %>
    <tr>
      <td><%= shop_id %></td>
      <td><%= points %></td>
      <td><%= rewards %></td>
      <td><%= points_to_rewards %></td>
    </tr>
    <% end %>
  </table>
</div>
